{
    "name": "d4rk0snet/adoption",
    "description": "Plugin gérant l'adoption d'un récif ou d'un corail",
    "type": "wordpress-plugin",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "D4rk0snet\\Adoption\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Grégory COLLIN",
            "email": "gregory@d4rk0s.net"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/doctrine.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/restapi.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/stripe.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:coralguardian/module-fiscalreceipt.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:coralguardian/module-certificate.git",
            "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:ds-hyperion/api2pdf.git",
            "no-api": true
        },
        {
          "type" : "vcs",
          "url": "git@github.com:coralguardian/module-giftcode.git",
          "no-api": true
        },
        {
          "type" : "vcs",
          "url": "git@github.com:coralguardian/module-order.git",
          "no-api": true
        },
        {
          "type" : "vcs",
          "url": "git@github.com:ds-hyperion/sendinblue.git",
          "no-api": true
        },
        {
            "type" : "vcs",
            "url": "git@github.com:coralguardian/module-naming_file_import.git",
            "no-api": true
        }
    ],
    "require": {
        "hyperion/doctrine" : "1.*",
        "hyperion/restapi": "1.*",
        "hyperion/stripe": "1.*",
        "hyperion/sendinblue": "1.*",
        "netresearch/jsonmapper": "^4.0",
        "ramsey/uuid": "^4.3",
        "ramsey/uuid-doctrine": "^1.8",
        "ext-json": "*",
        "d4rk0snet/fiscalreceipt": "dev-prod",
        "d4rk0snet/certificate": "dev-prod",
        "d4rk0snet/gift-code": "dev-prod",
        "d4rk0snet/naming-file-import": "dev-prod",
        "d4rk0snet/coral-order": "dev-prod"
    }
}
